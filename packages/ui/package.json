{
  "name": "@pui-vue/ui",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-s build:ui build:types",
    "build:ui": "esno scripts/build.ts",
    "build:types": "run-p types:*",
    "types:es": "tsc -p pui --declaration --emitDeclarationOnly --outDir dist/es",
    "types:umd": "tsc -p pui --declaration --emitDeclarationOnly --outDir dist/umd",
    "lint": "npx eslint"
  },
  "main": "dist/pui.umd.js",
  "module": "dist/pui.es.js",
  "dependencies": {
    "vue": "^3.2.16",
    "vue-router": "4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^2.60.1",
    "@types/node": "^16.11.6",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.1.4",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.1.1",
    "less": "^4.1.2",
    "prettier": "2.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
